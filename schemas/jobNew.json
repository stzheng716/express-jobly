{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/jobNew.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "salary": {
      "type": "integer",
      "min": 0
    },
    "equity": {
      "type": "number",
      "min": 0
    },
    "companyHandle": {
      "type": "string",
      "minLength": 1,
      "maxLength": 25
    }
  },
  "additionalProperties": false,
  "required": [
    "title",
    "salary",
    "equity",
    "companyHandle"
  ]
}
